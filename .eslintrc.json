{
  "extends": ["next", "next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 90,
        "tabWidth": 2,
        "bracketSpacing": true
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
